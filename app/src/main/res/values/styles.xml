<resources>
    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>

    </style>
    
    <style name="FABShape1" parent="ShapeAppearance.MaterialComponents.MediumComponent">
        <item name="cornerSize">50%</item>
        <item name="cornerFamilyTopLeft">cut</item>
        <item name="cornerFamilyTopRight">cut</item>
        <item name="cornerFamilyBottomLeft">rounded</item>
        <item name="cornerFamilyBottomRight">rounded</item>
    </style>

    <style name="FABShape2" parent="ShapeAppearance.MaterialComponents.MediumComponent">
        <item name="cornerSize">50%</item>
        <item name="cornerFamily">cut</item>
    </style>

    <style name="FABShape3" parent="ShapeAppearance.MaterialComponents.MediumComponent">
        <item name="cornerSize">35%</item>
    </style>

    <style name="FABShape4" parent="ShapeAppearance.MaterialComponents.MediumComponent">
        <item name="cornerSize">30%</item>
        <item name="cornerFamily">cut</item>
    </style>

    <style name="FABShape5" parent="ShapeAppearance.MaterialComponents.MediumComponent">
        <item name="cornerSize">50%</item>
        <item name="cornerSizeBottomRight">0dp</item>
        <item name="cornerFamilyTopLeft">rounded</item>
        <item name="cornerFamilyTopRight">rounded</item>
        <item name="cornerFamilyBottomLeft">rounded</item>
        <item name="cornerFamilyBottomRight">cut</item>
    </style>

    <style name="FABShape6" parent="ShapeAppearance.MaterialComponents.MediumComponent">
        <item name="cornerSize">0dp</item>
        <item name="cornerSizeTopLeft">80%</item>
        <item name="cornerFamily">cut</item>
        <item name="cornerFamilyTopLeft">rounded</item>
    </style>

</resources>